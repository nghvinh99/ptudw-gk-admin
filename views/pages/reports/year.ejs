<!DOCTYPE html>
<html lang="en">

<head>
	<title><%= title %></title>
	<% include meta %>
	<% include ../../elements/meta %>
</head>

<body>
	<% include ../../elements/navbar %>

	<div class="container">
		<div class="row tm-content-row">
			<div class="col-sm-12 col-md-12 col-lg-12 col-xl-12 tm-block-col">
			</br>
				<h2 class="tm-block-title">Biểu đồ năm</h2>
				<select class="custom-select" name="yearpicker" id="yearpicker">
					<option selected disabled hidden>Chọn năm...</option>
					<% years.forEach( (year) => { %>
						<option value="<%= year %>"><%= year %></option>
					<% }) %>
				</select>
				<canvas id="lineChart"></canvas>
			</div>
		</div>
		<div class="">
			<table class="table table-hover tm-table-small tm-product-table">
				<thead>
					<tr>
						<th scope="col">THÁNG</th>
						<th scope="col">SỐ LƯỢNG ĐƠN HÀNG</th>
						<th scope="col">TỔNG THU</th>
					</tr>
				</thead>
				<tbody id="analysisCol">
				</tbody>
			</table>
		</div>
	</div>

	<% include ../../elements/footer%>

	<% include scripts %>

	<% include ../../elements/scripts %>
	<script src="javascripts/ajax_report_year.js"></script>
</body>

</html>